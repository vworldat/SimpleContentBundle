{
    "name": "c33s/simple-content-bundle",
    "type": "symfony-bundle",
    "description": "routing-based menu system for symfony2",
    "homepage": "https://github.com/vworldat/SimpleContentBundle",
    "license": [
        "MIT",
        "GPL-3.0"
    ],
    "authors": [
        {
            "name": "David",
            "email": "office@web-emerge.com",
            "homepage": "http://web-emerge.com/"
        }
    ],
    "autoload": {
        "psr-0": {
            "C33s\\SimpleContentBundle": ""
        }
    },
    "target-dir": "C33s/SimpleContentBundle",
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.3.3",
        "propel/propel-bundle": "~1.2",
        "knplabs/knp-markdown-bundle": "~1.3"
    },
    "suggest": {
        "c33s/menu-bundle": ">=0.9.0",
        "cedriclombardot/admingenerator-generator-bundle": "~1.0"
    },
    "extra": {
        "c33s-building-blocks": [
            "C33s\\SimpleContentBundle\\BuildingBlock\\SimpleContentBuildingBlock"
        ]
    }
}
